Grammar

    0 $accept: LVM_METADATA $end

    1 LVM_METADATA: PARAMETERS VOLUME_GROUP
    2             | VOLUME_GROUP PARAMETERS

    3 VOLUME_GROUP: TOKEN_NAME "{" PARAMETERS VOLUME_GROUP_SECTIONS "}"

    4 VOLUME_GROUP_SECTIONS: PHYSICAL_VOLUME_SECTION LOGICAL_VOLUME_SECTION
    5                      | LOGICAL_VOLUME_SECTION PHYSICAL_VOLUME_SECTION
    6                      | PHYSICAL_VOLUME_SECTION

    7 PHYSICAL_VOLUME_SECTION: "physical_volumes" "{" SUB_SECTIONS_LIST "}"

    8 LOGICAL_VOLUME_SECTION: "logical_volumes" "{" LOGICAL_VOLUMES_LIST "}"

    9 LOGICAL_VOLUMES_LIST: LOGICAL_VOLUME LOGICAL_VOLUMES_LIST
   10                     | LOGICAL_VOLUME

   11 LOGICAL_VOLUME: TOKEN_NAME "{" PARAMETERS SUB_SECTIONS_LIST "}"

   12 SUB_SECTIONS_LIST: SUB_SECTIONS_LIST SUB_SECTION
   13                  | SUB_SECTION

   14 SUB_SECTION: TOKEN_NAME "{" PARAMETERS "}"

   15 PARAMETERS: PARAMETERS PARAMETER
   16           | PARAMETER

   17 PARAMETER: TOKEN_NAME "=" VALUE

   18 VALUE: TOKEN_INT_VALUE
   19      | TOKEN_FLOAT_VALUE
   20      | TOKEN_STRING_VALUE
   21      | ARRAY
   22      | PAIR

   23 ARRAY: "[" STRING_LIST "]"
   24      | "[" "]"

   25 STRING_LIST: TOKEN_STRING_VALUE "," STRING_LIST
   26            | TOKEN_STRING_VALUE

   27 PAIR: "[" TOKEN_STRING_VALUE "," TOKEN_INT_VALUE "]"


